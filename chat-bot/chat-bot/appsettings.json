{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=ChatBot;Integrated Security=true"
  },
  "JWTSettings": {
    "securityKey": "ChatBotSecredKey",
    "validIssuer": "ChatBotAPI",
    "validAudience": "https://localhost:44332",
    "expiryInMinutes": 30
  },
  "Logging": {
    "LogLevel": {
